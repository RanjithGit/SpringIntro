<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
          http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.1.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd
">

    
    <bean id="sound" class="com.learning.spring.Sound" >
        <constructor-arg ref="cat"></constructor-arg>
    </bean>
    
    <bean id="dog" class="com.learning.spring.Dog"> </bean>
    <bean id="cat" class="com.learning.spring.Cat"> </bean>
    <bean id="juggler" class="com.learning.manning.Juggler">
        <constructor-arg value="10"/> 
    </bean>
</beans>
